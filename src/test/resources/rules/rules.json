[
  {
    "tag": "001",
    "description": "The human readable ID",
    "conditions": [
      {
        "from": "$.hrid"
      }
    ]
  },
  {
    "tag": "041",
    "description": "Languages",
    "conditions": [
      {
        "from": "$.languages",
        "subfield": "a"
      }
    ]
  },
  {
    "tag": "245",
    "description": "Title",
    "conditions": [
      {
        "from": "$.title",
        "subfield": "a"
      }
    ]
  },
  {
    "tag": "247",
    "description": "Alternative Titles",
    "conditions": [
      {
        "from": "$.alternativeTitles[*].alternativeTitleTypeId",
        "subfield": "a",
        "translation": {
          "function": "set_alternative_title_type"
        }
      }
    ]
  },
  {
    "tag": "264",
    "description": "Contributor",
    "conditions": [
      {
        "description": "Place of publication",
        "from": "$.contributors[*].name",
        "subfield": "a"
      },
      {
        "description": "Publisher",
        "from": "$.contributors[*].contributorTypeText",
        "subfield": "b"
      }
    ]
  },
  {
    "tag": "856",
    "description": "Electronic access",
    "conditions": [
      {
        "description": "Place of publication",
        "from": "$.electronicAccess[*].relationshipId",
        "indicator": "2",
        "translation": {
          "function": "set_electronic_access_indicator",
          "parameter": {
            "from": "Resource",
            "value": 0
          }
        }
      },
      {
        "description": "Link text",
        "from": "$.electronicAccess[*].linkText",
        "subfield": "y"
      }
    ]
  },
  {
    "tag": "00x",
    "description": "Missing field just for the testing purposes",
    "conditions": [
      {
        "description": "Missing field just for the testing purposes",
        "from": "$.missing"
      }
    ]
  }
